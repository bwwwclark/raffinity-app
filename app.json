{
  "expo": {
    "name": "RAFinity",
    "slug": "raffinity-app",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rafinity-logo.png",
    "splash": {
      "image": "./assets/rafinity-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.rafinity.app",
      "buildNumber": "1.0.1",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/rafinity-logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/rafinity-logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "f597ac33-7135-451a-b9db-a18c091f23f6"
      }
    },
    "owner": "bwwwclark"
  }
