{
  "expo": {
    "name": "RAFinity",
    "slug": "raffinity-app",
    "scheme": "raffinity-app",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/rafinity-logo.png",
    "splash": {
      "image": "./assets/rafinity-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber":"1.0.25",
      "bundleIdentifier": "com.rafinity.app",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/rafinity-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rafinity.app"
    },
    "web": {
      "favicon": "./assets/rafinity-logo.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": false
        }
      ],
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "f597ac33-7135-451a-b9db-a18c091f23f6"
      }
    },
    "owner": "bwwwclark"
  }
}
